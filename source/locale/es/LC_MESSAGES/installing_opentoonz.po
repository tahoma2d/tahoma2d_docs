#: ../../source/installing_tahoma.rst:112
msgid "Installing on Linux"
msgstr "Instalación en Linux"

#: ../../source/installing_tahoma.rst:117
msgid "Arch Linux"
msgstr "Arch Linux"

#: ../../source/installing_tahoma.rst:119
msgid "``sudo pacman -S base-devel git``"
msgstr "``sudo pacman -S base-devel git``"

#: ../../source/installing_tahoma.rst:121
msgid "``git clone https://aur.archlinux.org/superlu.git``"
msgstr "``git clone https://aur.archlinux.org/superlu.git``"

#: ../../source/installing_tahoma.rst:123
msgid "``cd superlu``"
msgstr "``cd superlu``"

#: ../../source/installing_tahoma.rst:125
#: ../../source/installing_tahoma.rst:133
msgid "``makepkg -si``"
msgstr "``makepkg -si``"

#: ../../source/installing_tahoma.rst:127
msgid "``cd ..``"
msgstr "``cd ..``"

#: ../../source/installing_tahoma.rst:129
msgid "``git clone https://aur.archlinux.org/tahoma.git``"
msgstr "``git clone https://aur.archlinux.org/tahoma.git``"

#: ../../source/installing_tahoma.rst:131
msgid "``cd tahoma``"
msgstr "``cd tahoma``"

#: ../../source/installing_tahoma.rst:135
msgid "``export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/lib/tahoma/``"
msgstr "``export LD_LIBRARY_PATH=$LD_LIBRARY_PATH:/usr/lib/tahoma/``"

#: ../../source/installing_tahoma.rst:137
msgid ""
"``echo \"export LD_LIBRARY_PATH=\\$LD_LIBRARY_PATH:/usr/lib/tahoma\" >> "
"~/.bashrc``"
msgstr ""
"``echo \"export LD_LIBRARY_PATH=\\$LD_LIBRARY_PATH:/usr/lib/tahoma\" >> "
"~/.bashrc``"

#: ../../source/installing_tahoma.rst:142
msgid "Debian GNU/Linux"
msgstr "Debian GNU/Linux"

#: ../../source/installing_tahoma.rst:144
msgid "Download PGP signing key package."
msgstr "Descargar el paquete de firma PGP."

#: ../../source/installing_tahoma.rst:146
msgid ""
"``wget http://www.deb-multimedia.org/pool/main/d/deb-multimedia-keyring/deb-"
"multimedia-keyring_2016.8.1_all.deb``"
msgstr ""
"``wget http://www.deb-multimedia.org/pool/main/d/deb-multimedia-keyring/deb-"
"multimedia-keyring_2016.8.1_all.deb``"

#: ../../source/installing_tahoma.rst:148
msgid "Install PGP package."
msgstr "Instalar el paquete PGP."

#: ../../source/installing_tahoma.rst:150
msgid "``sudo dpkg -i deb-multimedia-keyring_2016.8.1_all.deb``"
msgstr "``sudo dpkg -i deb-multimedia-keyring_2016.8.1_all.deb``"

#: ../../source/installing_tahoma.rst:152
msgid "Add repository address to your sources list."
msgstr "Agregar la dirección del repositorio a la lista de orígenes."

#: ../../source/installing_tahoma.rst:154
msgid ""
"``sudo echo \"deb http://www.deb-multimedia.org stable main\" | tee -a /etc/"
"apt/sources.list`` Builds for testing and sid also avalaible."
msgstr ""
"``sudo echo \"deb http://www.deb-multimedia.org stable main\" | tee -a /etc/"
"apt/sources.list`` También hay disponibles versiones para testing y sid."

#: ../../source/installing_tahoma.rst:156
msgid "Debian packages avalaible only for amd64, arm64 and i386 architectures."
msgstr ""
"Los paquetes para Debian están disponibles únicamente para las arquitecturas "
"amd64, arm64 y i386."

#: ../../source/installing_tahoma.rst:158
msgid "Install Tahoma."
msgstr "Instalar Tahoma."

#: ../../source/installing_tahoma.rst:160
msgid "``sudo apt update && apt install tahoma tahoma-data``"
msgstr "``sudo apt update && apt install tahoma tahoma-data``"

#: ../../source/installing_tahoma.rst:162
msgid "For updating enter ``sudo apt update && apt upgrade``."
msgstr "Para actualizar, ingresar ``sudo apt update && apt upgrade``."

#: ../../source/installing_tahoma.rst:165
msgid "Gentoo, Funtoo"
msgstr "Gentoo, Funtoo"

#: ../../source/installing_tahoma.rst:167
msgid "``sudo emerge media-gfx/tahoma``"
msgstr "``sudo emerge media-gfx/tahoma``"

#: ../../source/installing_tahoma.rst:170
msgid "openSUSE"
msgstr "openSUSE"

#: ../../source/installing_tahoma.rst:172
msgid "``sudo zypper install tahoma``"
msgstr "``sudo zypper install tahoma``"

#: ../../source/installing_tahoma.rst:175
msgid "Solus"
msgstr "Solus"

#: ../../source/installing_tahoma.rst:177
msgid "``sudo eopkg install tahoma``"
msgstr "``sudo eopkg install tahoma``"

#: ../../source/installing_tahoma.rst:180
msgid "Void Linux"
msgstr "Void Linux"

#: ../../source/installing_tahoma.rst:182
msgid "``sudo xbps-install -S tahoma``"
msgstr "``sudo xbps-install -S tahoma``"

#: ../../source/installing_tahoma.rst:187
msgid "Universal binaries"
msgstr "Binarios universales"

#: ../../source/installing_tahoma.rst:190
msgid "Flatpak"
msgstr "Flatpak"

#: ../../source/installing_tahoma.rst:192
msgid "Installing."
msgstr "Instalar."

#: ../../source/installing_tahoma.rst:194
msgid "``flatpak install flathub io.github.Tahoma``"
msgstr "``flatpak install flathub io.github.Tahoma``"

#: ../../source/installing_tahoma.rst:196
msgid "Run."
msgstr "Ejecutar."

#: ../../source/installing_tahoma.rst:198
msgid "``flatpak run io.github.Tahoma``"
msgstr "``flatpak run io.github.Tahoma``"

#: ../../source/installing_tahoma.rst:203
msgid "Snap"
msgstr "Snap"

#: ../../source/installing_tahoma.rst:205
msgid "``sudo snap install tahoma``"
msgstr "``sudo snap install tahoma``"

